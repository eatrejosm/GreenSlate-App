<h2>@ViewBag.Title</h2>

<p data-bind="text:UserFlag"></p>

<div>
    <div class="form-group">
        <label class="control-label">First Name:</label>
        <span class="form-control" data-bind="text:FirstName"></span>
    </div>
    <div class="form-group">
        <label class="control-label">Last Name:</label>
        <span class="form-control" data-bind="text:LastName"></span>
    </div>
</div>

<table class="table table-striped">
    <tr>
        <th>Start Date</th>
        <th>End Date</th>
        <th>Credits</th>
    </tr>
    <tbody data-bind="foreach: CreateProjectItems">
        <tr>
            <td class="form-group"><span class="form-control input-sm" data-bind="text: StartDate"></span></td>
            <td class="form-group"><span class="form-control input-sm" data-bind="text: EndDate" ></span></td>
            <td class="form-group"><span class="form-control input-sm" data-bind="text: Credits" ></span></td>
        </tr>
    </tbody>
</table>

<p><a href="/" class="btn btn-default btn-sm">&laquo; Back to List</a></p>