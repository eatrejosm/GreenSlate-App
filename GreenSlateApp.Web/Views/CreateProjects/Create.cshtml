@model GreenSlateApp.Web.ViewModels.CreateProjectViewModel
@using System.Web.Script.Serialization

@{
    ViewBag.Title = "Create User";
}

@{ string data = new JavaScriptSerializer().Serialize(Model); }

@section scripts
{

    <script src="~/Scripts/knockout-3.5.0.js"></script>
    <script src="~/Scripts/knockout.mapping-latest.js"></script>

    <script src="~/Scripts/jquery.validate.js"></script>

    <script src="~/Scripts/createprojectviewmodel.js"></script>
    <script type="text/javascript">
    var createProjectViewModel = new CreateProjectViewModel(@Html.Raw(data));
    ko.applyBindings(createProjectViewModel);
    </script>
}

@Html.Partial("_EditCreateProject")
